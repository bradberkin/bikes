void setColumnAddress(unsigned char a, unsigned char b) {
  oledCommand(0x15); // Set Column Address
  oledData(a); // Default => 0x00
  oledData(b); // Default => 0x77
}

void setWriteRAM() {
  oledCommand(0x5C); // Enable MCU to Write DATA RAM
}

void setReadRAM() {
  oledCommand(0x5D); // Enable MCU to Read DATA RAM
}

void setRowAddress(unsigned char a, unsigned char b)
{
  oledCommand(0x75); // Set Row Start and End Address
  oledData(a); // Default => 0x00
  oledData(b); // Default => 0x7F
}

void setRemap(unsigned char a, unsigned char b) {
  oledCommand(0xA0); // Set Remap
  oledData(a); // Default => 0x00
  oledData(b); // Default => 0x00
}

void setDisplayStartLine(unsigned char a) {
  oledCommand(0xA1); // Set Display RAM Display Start Line
  oledData(a); // Default => 0x00
}

void setDisplayOffset(unsigned char a) {
  oledCommand(0xA2); // Set Verticle Shift
  oledData(a); // Default => 0x00
}

void displayMode(unsigned char a)
{
  oledCommand(a); // 0xA4 => Entire Display OFF 
                  // 0xA5 => Entire Display ON, all pixels Grayscale level 15
                  // 0xA6 => Normal Display (Default)
                  // 0xA7 => Inverse Display
}

void enablePartialDisplay(unsigned char a, unsigned char b) {
  oledCommand(0xA8);  // Turns ON partial mode 
  oledData(a); // Address of start row
  oledData(b); // Address of end row
}

void exitPartialDisplay() {
  oledCommand(0xA9); // Exit Partial Display Mode
}

void functionSelection(unsigned char a) {
  oledCommand(0xAB); // Function Selection
  oledData(a); // Default => 0x01
}

void setSleepMode(unsigned char a) {
  oledCommand(a);     // 0xAE => Sleep Mode ON
                  // 0xAF => Sleep Mode OFF
}

void setPhaseLength(unsigned char a) {
  oledCommand(0xB1); // Set Phase Length
  oledData(a); // Default => 0x97
}

void setDisplayClockOscillatorFrequency(unsigned char a) {
  oledCommand(0xB3); // Set Osc Frequency
  oledData(a);
}

void enableExternalVSL(unsigned char a, unsigned char b) {
  oledCommand(0xB4); // Enable External VSL
  oledData(a);
  oledData(b);
}

void setGPIO(unsigned char a) {
  oledCommand(0xB5); // Set GPIO
  oledData(a);
}

void setSecondPrechargePeriod(unsigned char a) {
  oledCommand(0xB6); // Set Second Precharge Period
  oledData(a);
}

void defaultGrayscaleCommand() {
  oledCommand(0xB9); // Set Default Grayscale
}

void setPrechargeVoltage(unsigned char a) {
  oledCommand(0xBB); // Set Precharge Voltage Level
  oledData(a); // Default => 0x17
}

void setVCOMHVoltage(unsigned char a) {
  oledCommand(0xBE); // Set Second Precharge Period
  oledData(a); // Default => 0x04
}

void setContrastControl(unsigned char a) {
  oledCommand(0xC1); // Set Contrast Control Level
  oledData(a); // Default => 0x7F
}

void masterContrastControl(unsigned char a) {
  oledCommand(0xC7); // Master Contrast Control
  oledData(a); // Default => 0x0F
}

void setMultiplexRatio(unsigned char a) {
  oledCommand(0xCA); // Set MUX Ratio
  oledData(a); // Default => 0x7F
}

void displayEnhancement(unsigned char a) {
  oledCommand(0xD1); // Display Enhancement
  oledData(a);
  oledData(0x20);
}

void setCommandLock(unsigned char a) {
  oledCommand(0xFD); // Set Lock/Unlock Commands
  oledData(a); // Default => 0x12
}

void oledInit() {
  setCommandLock(0x12); // Unlock commands for OLED
  setDisplayClockOscillatorFrequency(0xD1); // Set Display Clock / Oscillator Frequency
  setMultiplexRatio(0x3F); // Set Multiplex Ratio
  setDisplayOffset(0x00); // Set Display Offset 
  setDisplayStartLine(0x00); // Set Display Start Line
  setRemap(0x14,0x01); // Set Remap. (A[1]) & (A[4]) can be adjusted to flip display orientation
  setGPIO(0x00); // Set GPIO 
  functionSelection(0x00); // Function Selection
  enableExternalVSL(0xA0,0xB5); // Enable External VSL
  setContrastControl(0x7F); // Set Contrast Control
  masterContrastControl(0x0F); // Master Contrast Control
  defaultGrayscaleCommand(); // Default Linear Grayscale Table
  setPhaseLength(0x74); // Set Phase Length
  displayEnhancement(0xA2); // Display Enhancement
  setPrechargeVoltage(0x17); // Set Precharge Voltage
  setSecondPrechargePeriod(0x08); // Set Second Precharge Period
  setVCOMHVoltage(0x07); // Set VCOMH Voltage
  displayMode(0xA6); // Set Normal Display
  exitPartialDisplay(); // Exit Partial Display Mode
  setSleepMode(0xAF); // Sleep Mode OFF
}

void oledClear() {
  unsigned int r,c;
    
  setRowAddress(0,63);
  setColumnAddress(28,91);
  setWriteRAM();
  for (r=0; r<64; r++) {
    for (c=0; c<64; c++) {
      oledData(0x00);
      delayMicroseconds(100);
      oledData(0x00);
      delayMicroseconds(100);
    }
  }
}
